<?xml version="1.0"?>


<!DOCTYPE cache PUBLIC
  "-//GemStone Systems, Inc.//GemFire Declarative Caching 6.6//EN"
  "http://www.gemstone.com/dtd/cache6_6.dtd">
<cache>
	<!--bridge-server host="cod" port="24680" /-->
  <bridge-server port="HOST_PORT1" notify-by-subscription="false"/>
  <pdx read-serialized="true" />  
	<vm-root-region name="DistRegionAck">
		<region-attributes data-policy="partition">
     </region-attributes>
	</vm-root-region>
	<vm-root-region name="DistRegionNoAck">
		<region-attributes   scope="distributed-no-ack" mirror-type="keys-values">
    </region-attributes>
	</vm-root-region>
	<vm-root-region name="testregion">
		<region-attributes   scope="distributed-no-ack" mirror-type="keys-values">
    </region-attributes>
	</vm-root-region>
    <function-service>
    
  	<function>
  		<class-name>javaobject.ComparePdxTypes</class-name>
  	</function>  	
    <function>
  		<class-name>javaobject.IterateRegion</class-name>
  	</function>  	
  </function-service>
</cache> 
