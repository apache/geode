  "http://www.gemstone.com/dtd/cache7_0.dtd">
<cache>
  <bridge-server port="$PORT_NUM" max-threads="5"/>
  <region name="TestTCR1">
    <region-attributes data-policy="partition">
      <partition-attributes redundant-copies="1" total-num-buckets="10"/>
    </region-attributes>
  </region>
</cache> 
