<?xml version="1.0"?>
<!DOCTYPE cache PUBLIC
  "-//GemStone Systems, Inc.//GemFire Declarative Caching 6.0//EN"
  "http://www.gemstone.com/dtd/cache6_0.dtd">
<cache>
  <bridge-server port="$PORT_NUM" notify-by-subscription="true"/>
  <vm-root-region name="TestTCR1">
    <region-attributes data-policy="partition">
      <partition-attributes redundant-copies="2" total-num-buckets="20">
        <partition-resolver>
          <class-name>javaobject.ModPartitionResolver</class-name>
        </partition-resolver>
      </partition-attributes>
    </region-attributes>
  </vm-root-region>
 </cache>
