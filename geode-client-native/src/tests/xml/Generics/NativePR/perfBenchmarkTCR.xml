<?xml version="1.0" encoding="UTF-8"?>
<test-driver version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../testDefinition.xsd">

  <data name="timedInterval">100</data>
  <data name="cacheType">CLIENT</data>

  <data name="distinctKeys">
    <list>
      <item>100000</item>
    </list>
  </data>

  <data name="valueSizes">
    <list>
      <item>5000</item>
    </list>
  </data>

  <data name="numThreads">
    <list>
      <item>10</item>
    </list>
  </data>

  <data name="keyType">int</data>
  <data name="clientCount">2</data>

  <data name="ThinClientRegion">
    <snippet>
      <region name="TestTCR1">
        <region-attributes caching-enabled="true" client-notification="true"/>
      </region>
    </snippet>
  </data>
  <data name="regionSpec">ThinClientRegion</data>

  <client-set name="JCS1" hostGroup="CS"/>
  <client-set name="JCS2" hostGroup="CS"/>
  <client-set name="JCS3" hostGroup="CS"/>
  <client-set name="JCS4" hostGroup="CS"/>
  <client-set name="JCS5" hostGroup="CS"/>
  <client-set name="JCS6" hostGroup="CS"/>
  <client-set count="2" name="ThinClient"/>
  <client-set name="Feeder"/>

  <test name="ThinClientPerf" description="Thin Client perf test">
    <task name="SetupCacheServer" action="doRunProcess" container="utils" waitTime="5m">
      <data name="program">setupJavaServers</data>
      <data name="arguments">-c 6 NativePR/csNotifySubscription.xml</data>
      <client-set name="JCS1"/>
    </task>
    <task name="StartCacheServer 1" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">startJavaServers</data>
      <data name="arguments">1</data>
      <client-set name="JCS1"/>
    </task>
    <task name="StartCacheServer 2" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">startJavaServers</data>
      <data name="arguments">2</data>
      <client-set name="JCS2"/>
    </task>
    <task name="StartCacheServer 3" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">startJavaServers</data>
      <data name="arguments">3</data>
      <client-set name="JCS3"/>
    </task>
    <task name="StartCacheServer 4" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">startJavaServers</data>
      <data name="arguments">4</data>
      <client-set name="JCS4"/>
    </task>
    <task name="StartCacheServer 5" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">startJavaServers</data>
      <data name="arguments">5</data>
      <client-set name="JCS5"/>
    </task>
    <task name="StartCacheServer 6" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">startJavaServers</data>
      <data name="arguments">6</data>
      <client-set name="JCS6"/>
    </task>

    <task name="CreateRegion" action="doCreateRegion" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="5m" argTypes="Int32,byte[]">
      <client-set count="2" name="ThinClient"/>
      <client-set name="Feeder"/>
    </task>
    <task name="Register All Keys" action="doRegisterAllKeys" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="2m" argTypes="Int32,byte[]">
      <client-set count="2" name="ThinClient"/>
    </task>
    <task name="PopulateRegion" action="doPopulateRegion" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" argTypes="Int32,byte[]">
      <client-set name="Feeder"/>
    </task>
    <task name="Gets-identical" action="doGets" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" argTypes="Int32,byte[]">
      <client-set count="2" name="ThinClient"/>
    </task>
    <task name="Puts-identical" action="doPuts" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="15m" argTypes="Int32,byte[]">
      <client-set count="2" name="ThinClient"/>
    </task>
    <task name="Destroys-identical" action="doDestroys" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" argTypes="Int32,byte[]">
      <client-set count="2" name="ThinClient"/>
    </task>

    <task name="PopulateRegion" action="doPopulateRegion" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" argTypes="Int32,byte[]"> 
      <client-set name="Feeder"/>
    </task>
    <task name="Gets-overlap1" action="doGets" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" parallel="true" argTypes="Int32,byte[]">
      <client-set name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>75000</item>
        </list>
      </data>
    </task>
    <task name="Gets-overlap2" action="doGets" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" parallel="true" argTypes="Int32,byte[]">
      <client-set begin="2" count="1" name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>75000</item>
        </list>
      </data>
      <data name="keyIndexBegin">25000</data>
    </task>
    <task name="doSleep" action="doSleep" container="utils" waitTime="60">
      <client-set count="2" name="ThinClient"/>
      <data name="sleepTime">5</data>
    </task>
    <task name="Puts-overlap1" action="doPuts" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="15m" parallel="true" argTypes="Int32,byte[]">
      <client-set name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>75000</item>
        </list>
      </data>
    </task>
    <task name="Puts-overlap2" action="doPuts" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="15m" parallel="true" argTypes="Int32,byte[]">
      <client-set begin="2" count="1" name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>75000</item>
        </list>
      </data>
      <data name="keyIndexBegin">25000</data>
    </task>
    <task name="doSleep" action="doSleep" container="utils" waitTime="60">
      <client-set count="2" name="ThinClient"/>
      <data name="sleepTime">5</data>
    </task>
    <task name="Destroys-overlap1" action="doDestroys" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" parallel="true" argTypes="Int32,byte[]">
      <client-set name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>75000</item>
        </list>
      </data>
    </task>
    <task name="Destroys-overlap2" action="doDestroys" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" parallel="true" argTypes="Int32,byte[]">
      <client-set begin="2" count="1" name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>75000</item>
        </list>
      </data>
      <data name="keyIndexBegin">25000</data>
    </task>

    <task name="PopulateRegion" action="doPopulateRegion" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" argTypes="Int32,byte[]">
      <client-set name="Feeder"/>
    </task>
    <task name="Gets-non-overlap1" action="doGets" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" parallel="true" argTypes="Int32,byte[]">
      <client-set name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>50000</item>
        </list>
      </data>
    </task>
    <task name="Gets-non-overlap2" action="doGets" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" parallel="true" argTypes="Int32,byte[]">
      <client-set begin="2" count="1" name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>50000</item>
        </list>
      </data>
      <data name="keyIndexBegin">50000</data>
    </task>
    <task name="doSleep" action="doSleep" container="utils" waitTime="60">
      <client-set count="2" name="ThinClient"/>
      <data name="sleepTime">5</data>
    </task>
    <task name="Puts-non-overlap1" action="doPuts" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="15m" parallel="true" argTypes="Int32,byte[]">
      <client-set name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>50000</item>
        </list>
      </data>
    </task>
    <task name="Puts-non-overlap2" action="doPuts" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="15m" parallel="true" argTypes="Int32,byte[]">
      <client-set begin="2" count="1" name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>50000</item>
        </list>
      </data>
      <data name="keyIndexBegin">50000</data>
    </task>
    <task name="doSleep" action="doSleep" container="utils" waitTime="60">
      <client-set count="2" name="ThinClient"/>
      <data name="sleepTime">5</data>
    </task>
    <task name="Destroys-non-overlap1" action="doDestroys" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" parallel="true" argTypes="Int32,byte[]">
      <client-set name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>50000</item>
        </list>
      </data>
    </task>
    <task name="Destroys-non-overlap2" action="doDestroys" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="10m" parallel="true" argTypes="Int32,byte[]">
      <client-set begin="2" count="1" name="ThinClient"/>
      <data name="distinctKeys">
        <list>
          <item>50000</item>
        </list>
      </data>
      <data name="keyIndexBegin">50000</data>
    </task>

    <task name="Close Cache" action="doCloseCache" container="NewFwkLib.GemStone.GemFire.Cache.FwkLib.perftests" waitTime="2m" argTypes="Int32,byte[]"/>
    <task name="StopCacheServer 1" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">stopJavaServers</data>
      <data name="arguments">1</data>
      <client-set name="JCS1"/>
    </task>
    <task name="StopCacheServer 2" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">stopJavaServers</data>
      <data name="arguments">2</data>
      <client-set name="JCS2"/>
    </task>
    <task name="StopCacheServer 3" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">stopJavaServers</data>
      <data name="arguments">3</data>
      <client-set name="JCS3"/>
    </task>
    <task name="StopCacheServer 4" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">stopJavaServers</data>
      <data name="arguments">4</data>
      <client-set name="JCS4"/>
    </task>
    <task name="StopCacheServer 5" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">stopJavaServers</data>
      <data name="arguments">5</data>
      <client-set name="JCS5"/>
    </task>
    <task name="StopCacheServer 6" action="doRunProcess" container="utils" waitTime="10m" parallel="true">
      <data name="program">stopJavaServers</data>
      <data name="arguments">6</data>
      <client-set name="JCS6"/>
    </task>
  </test>

</test-driver>
