    </snippet>
  </data>
	
	<test name="Local" description="Local Entry Event test">
		<task name="doRegionOps" action="doRegionOperations" container="eventtest" waitTime="1200" timesToRun="15" continueOnError="true" threadCount="4">
			<data name="regionSpec">Local</data>
		</task>
	</test>
	
