            VALUE "OriginalFilename", "Gemstone.Gemfire.Cache.dll"
            VALUE "ProductName",      PRODUCT_NAME
            VALUE "ProductVersion",   PRODUCT_VERSION
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
