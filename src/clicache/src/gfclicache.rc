        BEGIN
            VALUE "CompanyName",      PRODUCT_VENDOR_NAME
            VALUE "FileDescription",  PRODUCT_NAME " .NET Library"
            VALUE "FileVersion",      PRODUCT_VERSION
            VALUE "InternalName",     "Gemstone.Gemfire.Cache"
